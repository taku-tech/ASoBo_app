<div class="container">
	<div class="row">
		<div class="col-xs-5">
			<%= form_with model: current_admin, url: admins_admin_path(current_admin), method: :patch, local: true do |f| %>
			<%= f.label :email, "メールアドレス" %>
			<%= f.email_field :email %>
			<%= f.submit "更新" %>
			<% end %>
		</div>
	</div>
</div>