<div class="container">
	<div class="row">
		<div class="col-xs-8 col-xs-offset-2">
			<h2>成績表</h2>
			<table class="table">
				<thead>
					<tr>
						<th>テスト実施日</th>
						<th>難易度</th>
						<th>点数</th>
					</tr>
				</thead>
				<tbody>
					<% @results.each do |result| %>
					<tr>
						<td><%= link_to result.created_at.strftime("%Y/%m/%d %H:%M"), result_path(result.id) %></td>
						<td>レベル</td>
						<td>0000</td>
					</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>